 
 // Sequence equation problem in Java
 
 List<Integer> result = new ArrayList<>();
    int x=1,y=0;
    while(x<=p.size()){
        for(int i=0;i<p.size();i++){
            if(p.get(i)==x){
                y=i+1;
                break;
            }
        }
        for(int i=0;i<p.size();i++){
            if(p.get(i)==y){
                y=i+1;
                break;
            }
        }
        
    
    result.add(y);
    x++;

    }
    return result;
    }

}
