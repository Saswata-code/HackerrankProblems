// Repeated String Problem in Hackerrank
long len=s.length();
    long repeatations= n/len;
    long count=0;
    long rem=n%len;
    long noOfA=0;
    for(int i=0;i<len;i++){
        if(s.charAt(i)=='a'){
            count++;
        }
    }
    noOfA=count*repeatations;
    for(int i=0;i<rem;i++){
        if(s.charAt(i)=='a'){
            noOfA++;
        }
    }
    
    return noOfA;
    
